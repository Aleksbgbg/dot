[alias]
	ame = commit --amend --no-edit
	bla = blame -w -C -C -C
	com = commit -v
	dif = diff --staged
	fix = commit --fixup
	lol = log --graph --pretty=format:'%C(auto)%h%d%Creset %C(cyan)(%cr)%Creset %C(green)%cn <%ce>%Creset %s'
	lold = log --graph --pretty=format:\"%C(auto)%h%d%Creset %C(cyan)(%ci)%Creset %C(green)%cn <%ce>%Creset %s\"
	swi = switch -c
[color]
	ui = auto
[core]
	autocrlf = input
	ignorecase = false
[diff]
	algorithm = patience
[init]
	defaultBranch = main
[gc]
	pruneexpire = 90 days
[rerere]
	enabled = true
